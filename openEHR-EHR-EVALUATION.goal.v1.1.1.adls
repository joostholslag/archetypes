    details = <
        ["fi"] = <
            language = <[ISO_639-1::fi]>
            purpose = <"*To record details about a health-related goal and any associated targets and deadlines.(en)">
            keywords = <"*target(en)", "*goal(en)", "*plan(en)", "*outcome(en)">
            use = <"*Use to record a health-related goal, as well as one or more specific, measurable targets that will support assessment of success or allow for recording difficulties in achieving the target.
    
    This archetype is intended for use as a component of a care plan, COMPOSITION.care_plan, and will be versioned over time as any changes are made. For example, if a new goal is added to a care plan, or or any other change made, the data will be added/updated and the Care plan saved as a new version. It can be used in any other COMPOSITION or SECTION archetypes, where clinically relevant.(en)">
            misuse = <"">
        >
        ["es-ar"] = <
            language = <[ISO_639-1::es-ar]>
            purpose = <"El registro de metas relativas a la salud y demás objetivos asociados, y sus fechas límite.">
            keywords = <"objetivo", "meta", "plan", "desenlace">
            use = <"Utilizado para registrar metas relacionadas a la salud, asi como uno o mas objetivos específicos mensurables que permitan la evaluación del éxito o el registro de las dificultades para alcanzar el objetivo.
    Este arquetipo pretende ser un componente de un plan de cuidados, COMPOSITION.care_plan, y será versionado a través del tiempo a medida que se realicen cambios. Por ejemplo, si una nueva meta es agregada a un plan de cuidados, o se realiza cualquier otro cambio, los datos serán agregados o actualizados y el plan almacenado como una nueva versión. Puede ser utilizado en cualquier otro arquetipo COMPOSITION o SECTION cuando sea clínicamente relevante.">
            misuse = <"">
        >
        ["pt-br"] = <
            language = <[ISO_639-1::pt-br]>
            purpose = <"Para gravar detalhes sobre um objetico de saúde e as metas e prazos associados.">
            keywords = <"objetivo", "alvo", "plano", "resultado">
            use = <"Use para gravar uma meta relacionada à saúde, bem como um ou mais alvos específicos e mensuráveis que irão apoiar a avaliação do sucesso ou permitir o registro de dificuldades em atingir o alvo.
    
    Este arquétipo é destinado ao uso como um componente de um plano de cuidados, COMPOSITION.care_plan e vai ser versionado ao longo do tempo, como todas as alterações são feitas. Por exemplo, se uma nova meta é adicionada a um plano de cuidados, ou a qualquer outra alteração feita, os dados serão adicionados/atualizados e o plano de cuidados salvo como uma nova versão. Ele pode ser usado em qualquer outro arquétipo COMPOSITION ou SEÇTION, quando clinicamente relevante.">
            misuse = <"">
        >
        ["ar-sy"] = <
            language = <[ISO_639-1::ar-sy]>
            purpose = <"لتسجيل الأهداف العامة و الأهداف المُحَدَّدة القابلة للقياس (و ربما الإضافية) المؤدية إلى الهدف العام.">
            keywords = <"الهدف", ...>
            use = <"">
            misuse = <"">
            copyright = <"© openEHR Foundation">
        >
        ["en"] = <
            language = <[ISO_639-1::en]>
            purpose = <"To record details about a health-related goal and any associated targets and deadlines.">
            keywords = <"target", "goal", "plan", "outcome">
            use = <"Use to record a health-related goal, as well as one or more specific, measurable targets that will support assessment of success or allow for recording difficulties in achieving the target.
    
    This archetype is intended for use as a component of a care plan, COMPOSITION.care_plan, and will be versioned over time as any changes are made. For example, if a new goal is added to a care plan, or or any other change made, the data will be added/updated and the Care plan saved as a new version. It can be used in any other COMPOSITION or SECTION archetypes, where clinically relevant.">
            misuse = <"">
            copyright = <"© openEHR Foundation">
        >
        ["nl"] = <
            language = <[ISO_639-1::nl]>
            purpose = <"Voor het vastleggen van gegevens over een gezondheidsgerelateerd doel en andere bijbehorende doelstellingen en deadlines.">
            keywords = <"doelstelling", "doel", "plan", "uitkomst", "streefwaarde">
            use = <"Gebruik voor het vastleggen van een gezondheidsgerelateerd doel, als ook een of meerdere specifieke, meetbare doelstellingen die het beoordelen van een resultaat mogelijk maken of voor het mogelijke maken van het vastleggen van belemmeringen in het bereiken van de doelstelling.
    
    Dit archetype is bedoeld voor gebruik als onderdeel van een zorgplan: COMPOSITION.care_plan en zal aparte versies krijgen wanneer er iets wijzigt. Bijvoorbeeld wanneer een nieuw doel toegevoegd wordt aan het zorgplan of als er andere veranderingen zijn, de data zal toegevoegd/gewijzigd worden in het zorgplan, en hiervan wordt een nieuwe versie opgeslagen. Het kan gebruikt worden in elk andere COMPOSTION of SECTION archetype, wanneer klinische relevant.">
            misuse = <"">
        >
    >

definition
    EVALUATION[id1] matches {    -- Goal
        data matches {
            ITEM_TREE[id2] matches {    -- Tree
                items cardinality matches {1..*} matches {
                    ELEMENT[id3] matches {    -- Goal name
                        value matches {
                            DV_TEXT[id9001] 
                        }
                    }
                    ELEMENT[id13] occurrences matches {0..1} matches {    -- Goal description
                        value matches {
                            DV_TEXT[id9028] 
                        }
                    }
                    ELEMENT[id11] matches {    -- Clinical indication
                        value matches {
                            DV_TEXT[id9029] 
                        }
                    }
                    ELEMENT[id26] occurrences matches {0..1} matches {    -- Goal start date
                        value matches {
                            DV_DATE_TIME[id9037] 
                        }
                    }
                    ELEMENT[id4] occurrences matches {0..1} matches {    -- Goal proposed date
                        value matches {
                            DV_DATE[id9002] 
                        }
                    }
                    ELEMENT[id5] occurrences matches {0..1} matches {    -- Goal end date
                        value matches {
                            DV_DATE[id9003] 
                        }
                    }
                    ELEMENT[id14] occurrences matches {0..1} matches {    -- Goal outcome
                        value matches {
                            DV_TEXT[id9020] 
                            DV_CODED_TEXT[id9030] matches {
                                defining_code matches {[ac9026]}    -- Goal outcome (synthesised)
                            }
                        }
                    }
                    ELEMENT[id23] occurrences matches {0..1} matches {    -- Goal comment
                        value matches {
                            DV_TEXT[id9031] 
                        }
                    }
                    allow_archetype CLUSTER[id29] matches {     -- Readiness for change
                        include
                            archetype_id/value matches {/openEHR-EHR-CLUSTER\.change(-[a-zA-Z0-9_]+)*\.v1.*/}
                    }
                    CLUSTER[id6] matches {    -- Target
                        items matches {
                            ELEMENT[id12] occurrences matches {0..1} matches {    -- Target name
                                value matches {
                                    DV_TEXT[id9032] 
                                }
                            }
                            ELEMENT[id8] occurrences matches {0..1} matches {    -- Target
                                value matches {
                                    DV_INTERVAL<DV_COUNT>[id9013] matches {
                                        upper matches {
                                            DV_COUNT[id9014] 
                                        }
                                        lower matches {
                                            DV_COUNT[id9015] 
                                        }
                                    }
                                    DV_INTERVAL<DV_QUANTITY>[id9016] matches {
                                        upper matches {
                                            DV_QUANTITY[id9017] matches {
                                                property matches {[at9000]}    -- self
                                            }
                                        }
                                        lower matches {
                                            DV_QUANTITY[id9018] matches {
                                                property matches {[at9000]}    -- self
                                            }
                                        }
                                    }
                                    DV_DURATION[id9022] 
                                    DV_QUANTITY[id9023] 
                                    DV_COUNT[id9024] 
                                    DV_PROPORTION[id9025] 
                                    DV_TEXT[id9026] 
                                }
                            }
                            ELEMENT[id25] occurrences matches {0..1} matches {    -- Target description
                                value matches {
                                    DV_TEXT[id9033] 
                                }
                            }
                            ELEMENT[id7] occurrences matches {0..1} matches {    -- Target path
                                value matches {
                                    DV_URI[id9004] 
                                }
                            }
                            ELEMENT[id9] occurrences matches {0..1} matches {    -- Target proposed date
                                value matches {
                                    DV_DATE[id9011] 
                                }
                            }
                            ELEMENT[id10] occurrences matches {0..1} matches {    -- Target end date
                                value matches {
                                    DV_DATE[id9012] 
                                }
                            }
                            ELEMENT[id19] occurrences matches {0..1} matches {    -- Target outcome
                                value matches {
                                    DV_TEXT[id9034] 
                                    DV_CODED_TEXT[id9035] matches {
                                        defining_code matches {[ac9027]}    -- Target outcome (synthesised)
                                    }
                                }
                            }
                            ELEMENT[id24] occurrences matches {0..1} matches {    -- Target comment
                                value matches {
                                    DV_TEXT[id9036] 
                                }
                            }
                        }
                    }
                }
            }
        }
        protocol matches {
            ITEM_TREE[id27] matches {    -- Tree
                items cardinality matches {0..*; unordered} matches {
                    ELEMENT[id30] occurrences matches {0..1} matches {    -- Last updated
                        value matches {
                            DV_DATE_TIME[id9038] 
                        }
                    }
                    allow_archetype CLUSTER[id28] matches {     -- Extension
                        include
                            archetype_id/value matches {/.*/}
                    }
                }
            }
        }
    }

